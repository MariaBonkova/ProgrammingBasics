package Lab;

import java.util.Scanner;

public class P7ProjectsCreation {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        //вход
        String argName = scanner.nextLine();
        int project = Integer.parseInt(scanner.nextLine());
        // изчисления
        int hours = project * 3;

        System.out.println("The architect "+argName+ " will need " +hours+" hours to complete "+project +" project/s."
        );
    }
}
