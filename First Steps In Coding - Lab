package Lab;

import java.util.Scanner;

public class P9YardGreening {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        
        double sqm = Double.parseDouble(scanner.nextLine());
        double yard = sqm * 7.61;
        double discount = yard * 0.18;
        double finalPrice =yard - discount;
        System.out.println("The final price is: "+finalPrice+" lv.");
        System.out.println("The final discount is: "+discount+" lv."
        );
    }
}

